<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.greenfarm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="org.litepal.LitePalApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Greenfarm">
        <activity android:name=".ui.mine.myFarm.farmer.MyFarmActivity"></activity>
        <activity android:name=".ui.mine.setting.ChangeTelephoneActivity" />
        <activity android:name=".ui.mine.setting.ChangeUsernameActivity" />
        <activity android:name=".ui.mine.setting.ChangePasswordActivity" />
        <activity android:name=".ui.mine.setting.SettingActivity" />
        <activity android:name=".ui.main.MainActivity" />
        <activity android:name=".ui.register.RegisterActivity" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>